<property name="Phone UI" active="true" image="/system/etc/paranoid/preferences/images/phone.png" color="#f4b122">

        <description><![CDATA[Phone workspace<br/>180 DPI<br/>Smaller interface]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="180"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="360"/>
        <setting type="Hybrid" target="com.android.systemui.navbar.dpi" value="100"/>
        <setting type="Hybrid" target="com.android.systemui.statusbar.dpi" value="100"/>
        <setting type="Android" target="status_bar_clock" value="1"/>
        <setting type="Android" target="max_notification_icons" value="5"/>

</property>
